<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <templates-system j:dependencies="contentRetrieval default default-skins facets grid profile serverSettings skins tabularList tags"
                          j:installedModules="Jahia System Templates"
                          j:moduleType="templatesSet"
                          j:title="Jahia System Templates"
                          jcr:primaryType="jnt:module">
            <portlets jcr:primaryType="jnt:portletFolder"/>
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder">
                <profile-page-items jcr:primaryType="jnt:contentFolder">
                    <j:translation_en jcr:language="en"
                                      jcr:mixinTypes="mix:title"
                                      jcr:primaryType="jnt:translation"
                                      jcr:title="##resourceBundle(system.profilePageItems)##"/>
                    <home divClass="user-home"
                          jcr:primaryType="jnt:componentLink"
                          targetTemplate="user-home">
                        <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="##resourceBundle(home)##"/>
                    </home>
                    <my-settings divClass="user-edit-details"
                                 jcr:primaryType="jnt:componentLink"
                                 targetTemplate="user-edit-details">
                        <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="##resourceBundle(system.mySettings)##"/>
                    </my-settings>
                    <my-pages divClass="user-content"
                              jcr:primaryType="jnt:componentLink"
                              targetTemplate="user-content">
                        <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="##resourceBundle(system.myPages)##"/>
                    </my-pages>
                    <my-content divClass="user-sites"
                                jcr:primaryType="jnt:componentLink"
                                targetTemplate="user-content-manager">
                        <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="##resourceBundle(system.myContent)##"/>
                    </my-content>
                    <my-web-projects divClass="user-sites"
                                     jcr:primaryType="jnt:componentLink"
                                     targetTemplate="user-sites">
                        <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="##resourceBundle(system.myWebProjects)##"/>
                    </my-web-projects>
                </profile-page-items>
            </contents>
            <templates jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                        <default jcr:primaryType="jnt:folder">
                            <logo-jahia.png j:height="30"
                                            j:width="57"
                                            jcr:mixinTypes="jmix:image"
                                            jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            </logo-jahia.png>
                        </default>
                    </themes>
                    <icons jcr:primaryType="jnt:folder">
                        <my-settings.png j:height="64"
                                         j:width="64"
                                         jcr:mixinTypes="jmix:image"
                                         jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </my-settings.png>
                        <my-pages.png j:height="64"
                                      j:width="64"
                                      jcr:mixinTypes="jmix:image"
                                      jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </my-pages.png>
                        <my-sites.png j:height="64"
                                      j:width="64"
                                      jcr:mixinTypes="jmix:image"
                                      jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </my-sites.png>
                        <myprofile.png j:height="48"
                                       j:width="48"
                                       jcr:mixinTypes="jmix:image"
                                       jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </myprofile.png>
                        <mybookmarks.png j:height="48"
                                         j:width="48"
                                         jcr:mixinTypes="jmix:image"
                                         jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </mybookmarks.png>
                        <mysites.png j:height="48"
                                     j:width="48"
                                     jcr:mixinTypes="jmix:image"
                                     jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </mysites.png>
                        <mycontent.png j:height="48"
                                       j:width="48"
                                       jcr:mixinTypes="jmix:image"
                                       jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </mycontent.png>
                        <myspace.png j:height="48"
                                     j:width="48"
                                     jcr:mixinTypes="jmix:image"
                                     jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </myspace.png>
                    </icons>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <base j:view="system"
                      jcr:mixinTypes="jmix:theme"
                      jcr:primaryType="jnt:template">
                    <home jcr:primaryType="jnt:pageTemplate"/>
                    <pagecontent jcr:primaryType="jnt:contentList">
                        <content jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:language="en"
                                              jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation"
                                              jcr:title="##resourceBundle(system.content)##"/>
                            <pagecontent j:allowedTypes="" jcr:primaryType="jnt:area"/>
                        </content>
                        <footer colNames="footer-colContent"
                                customColumn="16"
                                divID="footer"
                                jcr:primaryType="jnt:customRow">
                            <footer-colContent jcr:primaryType="jnt:contentList">
                                <copyright jcr:primaryType="jnt:bigText">
                                    <j:translation_en jcr:language="en"
                                                      jcr:primaryType="jnt:translation"
                                                      text="&lt;p style=&#34;text-align: center;&#34;&gt;  &amp;copy; 2002-2014 Jahia Solutions Group SA - ##resourceBundle(org.jahia.copyright)##&lt;/p&gt; "/>
                                </copyright>
                            </footer-colContent>
                        </footer>
                    </pagecontent>
                    <user j:applyOn="jnt:user"
                          j:defaultTemplate="true"
                          j:hiddenTemplate="false"
                          j:priority="10"
                          jcr:primaryType="jnt:contentTemplate">
                        <pagecontent j:style="blue"
                                     jcr:mixinTypes="jmix:style"
                                     jcr:primaryType="jnt:contentList">
                            <custom-rows-960gs customColumn="16" divID="content" jcr:primaryType="jnt:customRow">
                                <custom-rows-960gs-col1 jcr:primaryType="jnt:contentList">
                                    <public-profile j:skin="skins.box8"
                                                    j:style="blue"
                                                    jcr:mixinTypes="jmix:skinnable jmix:style"
                                                    jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en"
                                                          jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation"
                                                          jcr:title="##resourceBundle(system.publicProfile)##"/>
                                        <userDetails j:about="true"
                                                     j:birthDate="true"
                                                     j:email="true"
                                                     j:facebookID="true"
                                                     j:firstName="true"
                                                     j:function="true"
                                                     j:gender="true"
                                                     j:lastName="true"
                                                     j:linkedinID="true"
                                                     j:organization="true"
                                                     j:picture="true"
                                                     j:skypeID="true"
                                                     j:title="true"
                                                     j:twitterID="true"
                                                     j:view="default"
                                                     jcr:mixinTypes="jmix:renderable"
                                                     jcr:primaryType="jnt:userDetails"/>
                                    </public-profile>
                                </custom-rows-960gs-col1>
                            </custom-rows-960gs>
                        </pagecontent>
                    </user>
                </base>
                <baseempty j:hiddenTemplate="false"
                           j:view="system.empty"
                           jcr:primaryType="jnt:template">
                    <j:translation_en jcr:language="en"
                                      jcr:mixinTypes="mix:title"
                                      jcr:primaryType="jnt:translation"
                                      jcr:title="baseEmpty"/>
                    <viewContent j:applyOn="jnt:file jnt:nodeType"
                                 j:defaultTemplate="true"
                                 j:hiddenTemplate="true"
                                 j:invertCondition="false"
                                 j:priority="15"
                                 j:requireLoggedUser="false"
                                 j:requirePrivilegedUser="false"
                                 jcr:primaryType="jnt:contentTemplate">
                        <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="##resourceBundle(system.viewContent)##"/>
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <main-resource-display j:mainResourceView="display" jcr:primaryType="jnt:mainResourceDisplay"/>
                        </pagecontent>
                    </viewContent>
                </baseempty>
                <moduletemplate j:applyOn="jnt:module"
                                j:defaultTemplate="true"
                                j:hiddenTemplate="false"
                                j:view="serverSettings"
                                jcr:primaryType="jnt:contentTemplate">
                    <j:translation_en jcr:language="en"
                                      jcr:mixinTypes="mix:title"
                                      jcr:primaryType="jnt:translation"
                                      jcr:title="moduleTemplate"/>
                    <pagecontent jcr:primaryType="jnt:contentList">
                        <manage-modules jcr:primaryType="jnt:serverSettingsManageModules">
                            <j:translation_en jcr:language="en"
                                              jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation"
                                              jcr:title="manage-modules"/>
                        </manage-modules>
                    </pagecontent>
                </moduletemplate>
            </templates>

            <categories jcr:primaryType="jnt:category" jcr:title="Categories"/>
            <home j:isHomePage="true"
                  j:templateName="home"
                  jcr:primaryType="jnt:page">
                <j:translation_en jcr:language="en"
                                  jcr:mixinTypes="mix:title"
                                  jcr:primaryType="jnt:translation"
                                  jcr:title="##resourceBundle(home)##"/>
            </home>
        </templates-system>
    </modules>
</content>
